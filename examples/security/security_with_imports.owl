[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If a required objective by a FG is in ERROR -> fg in error"^^xsd:string ;
   rdfs:label "S6c"^^xsd:string ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate suave:o_status ;
                           swrl:argument1 <http://ros/mros#o> ;
                           swrl:argument2 "IN_ERROR_FR"^^xsd:string
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate suave:FunctionGrounding ;
                                      swrl:argument1 <http://ros/mros#fg>
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate suave:requiresO ;
                                                 swrl:argument1 <http://ros/mros#fg> ;
                                                 swrl:argument2 <http://ros/mros#o>
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate suave:Objective ;
                                                            swrl:argument1 <http://ros/mros#o>
                                                          ] ;
                                                rdf:rest rdf:nil
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate suave:fg_status ;
                           swrl:argument1 <http://ros/mros#fg> ;
                           swrl:argument2 "IN_ERROR_FR"^^xsd:string
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

