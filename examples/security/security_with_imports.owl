<http://www.metacontrol.org/security> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.metacontro.org/security#hasAlertLevel
<http://www.metacontro.org/security#hasAlertLevel> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                                   rdfs:range security:AlertLevel .


###  http://www.metacontrol.org/security#maxAlertLevel
security:maxAlertLevel rdf:type owl:ObjectProperty ;
                       rdfs:domain suave:FunctionDesign ;
                       rdfs:range security:AlertLevel .


###  http://www.metacontrol.org/suave#fd_error_log
suave:fd_error_log rdf:type owl:ObjectProperty ;
                   rdfs:domain suave:FunctionDesign ;
                   rdfs:range suave:Objective ;
                   rdfs:comment "contains all objectives for which an FG instance of the FD has failed" .


###  http://www.metacontrol.org/suave#hasNFR
suave:hasNFR rdf:type owl:ObjectProperty ;
             rdfs:domain suave:Objective ;
             rdfs:range suave:QAvalue .


###  http://www.metacontrol.org/suave#hasQAestimation
suave:hasQAestimation rdf:type owl:ObjectProperty ;
                      rdfs:domain suave:FunctionDesign ;
                      rdfs:range suave:QAvalue .


###  http://www.metacontrol.org/suave#hasQAvalue
suave:hasQAvalue rdf:type owl:ObjectProperty ;
                 rdfs:domain suave:FunctionGrounding ;
                 rdfs:range suave:QAvalue .


###  http://www.metacontrol.org/suave#isQAtype
suave:isQAtype rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain suave:QAvalue ;
               rdfs:range suave:QualityAttributeType .


###  http://www.metacontrol.org/suave#requiresC
suave:requiresC rdf:type owl:ObjectProperty ;
                rdfs:domain suave:FunctionDesign ;
                rdfs:range suave:Component .


###  http://www.metacontrol.org/suave#requiresO
suave:requiresO rdf:type owl:ObjectProperty ;
                rdfs:domain suave:FunctionGrounding ;
                rdfs:range suave:Objective ;
                rdfs:comment "A FunctionGrounding may need an Objective to be solved (by someone else) as a requirement to realise its target Objective" .


###  http://www.metacontrol.org/suave#solvesF
suave:solvesF rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain suave:FunctionDesign ;
              rdfs:range suave:Function ;
              rdfs:comment "a FunctionDesign solves a Function" .


###  http://www.metacontrol.org/suave#solvesO
suave:solvesO rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain suave:FunctionGrounding ;
              rdfs:range suave:Objective ;
              rdfs:comment "A FunctionGrounding is realising an Objective" .


###  http://www.metacontrol.org/suave#typeF
suave:typeF rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain suave:Objective ;
            rdfs:range suave:Function ;
            rdfs:comment "relates a FunctionGrounding/Objective with a FunctionDesign/Function" .


###  http://www.metacontrol.org/suave#typeFD
suave:typeFD rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain suave:FunctionGrounding ;
             rdfs:range suave:FunctionDesign .


#################################################################
#    Data properties
#################################################################

###  http://www.metacontrol.org/security#alert_severity
security:alert_severity rdf:type owl:DatatypeProperty ;
                        rdfs:domain security:AlertLevel ;
                        rdfs:range xsd:decimal .


###  http://www.metacontrol.org/suave#c_status
suave:c_status rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain suave:Component ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "FAILURE" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "OK" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "RECOVERED" ;
                                                              rdf:rest rdf:nil
                                                            ]
                                                 ]
                                      ]
                          ] ;
               rdfs:comment "operational status of a component. For the moment \"false\"=\"ERROR\", later we can expand the status to an enumeration" .


###  http://www.metacontrol.org/suave#fd_efficacy
suave:fd_efficacy rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain suave:FunctionDesign ;
                  rdfs:range xsd:decimal .


###  http://www.metacontrol.org/suave#fd_realisability
suave:fd_realisability rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain suave:FunctionDesign ;
                       rdfs:range xsd:boolean .


###  http://www.metacontrol.org/suave#fg_status
suave:fg_status rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain suave:FunctionGrounding ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "IN_ERROR_COMPONENT" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "IN_ERROR_FR" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "IN_ERROR_NFR" ;
                                                               rdf:rest rdf:nil
                                                             ]
                                                  ]
                                       ]
                           ] .


###  http://www.metacontrol.org/suave#hasValue
suave:hasValue rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain suave:QAvalue ;
               rdfs:range xsd:decimal .


###  http://www.metacontrol.org/suave#o_always_improve
suave:o_always_improve rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain suave:Objective ;
                       rdfs:range xsd:boolean .


###  http://www.metacontrol.org/suave#o_status
suave:o_status rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain suave:Objective ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "IN_ERROR_COMPONENT" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "IN_ERROR_FR" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "IN_ERROR_NFR" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "IN_PROGRESS" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "UNGROUNDED" ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "UNREACHABLE" ;
                                                                                               rdf:rest rdf:nil
                                                                                             ]
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                      ]
                          ] .


###  http://www.metacontrol.org/suave#o_updatable
suave:o_updatable rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain suave:Objective ;
                  rdfs:range xsd:boolean .


###  http://www.metacontrol.org/suave#qa_comparison_operator
suave:qa_comparison_operator rdf:type owl:DatatypeProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain suave:QualityAttributeType ;
                             rdfs:range [ rdf:type rdfs:Datatype ;
                                          owl:oneOf [ rdf:type rdf:List ;
                                                      rdf:first "<" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "<=" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first ">" ;
                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                       rdf:first ">=" ;
                                                                                       rdf:rest rdf:nil
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                        ] .


###  http://www.metacontrol.org/suave#qa_critical
suave:qa_critical rdf:type owl:DatatypeProperty ;
                  rdfs:domain suave:QualityAttributeType ;
                  rdfs:range xsd:boolean .


